    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.4",
    "ts-node": "~8.9.0",
    "tsickle": "^0.37.0",
    "tslib": "^1.11.1",
    "tslint": "~6.1.1",
    "typescript": "~3.5.3"
